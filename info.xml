    <url desc="Main Extension Page">http://civicrm.org/extensions/civimobile</url>
    <url desc="Documentation">https://github.com/webaccess/com.webaccessglobal.module.civimobile</url>
    <url desc="Support">http://forum.civicrm.org/index.php/board,57.0.html</url>
    <url desc="Licensing">http://civicrm.org/licensing</url>
  </urls>
  <license>AGPL</license>
  <maintainer>
    <author>Web Access</author>
    <email>info@webaccessglobal.com</email>
  </maintainer>
  <releaseDate>2024-07-01</releaseDate>
  <version>3.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.74</ver>
  </compatibility>
  <comments>This version developed by Adam Hillier as a GSOC 2016 project.</comments>
  <civix>
    <namespace>CRM/Civimobile</namespace>
    <format>23.02.1</format>
